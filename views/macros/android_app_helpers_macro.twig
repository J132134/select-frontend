{% macro android_app_helpers(BASE_URL) %}
<script>
  var isLoginRequired = function(url) {
    return false;
  };
  var isExternalLink = function(url) {
    var innerLinkRegex = /^{{ BASE_URL|slice(2) }}|^\/\/{{ BASE_URL|slice(2) }}|^https:\/\/{{ BASE_URL|slice(2) }}|^\/(?=\w)/g;
    return !innerLinkRegex.test(url);
  };
</script>
{% endmacro %}
