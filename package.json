  "scripts": {
    "clean": "rm -rf dist",
    "dev": "postcss src/css/*.css -d dist/css & cpx 'src/fonts/**' dist/fonts & webpack",
    "dev:mock": "postcss src/css/*.css -d dist/css -w & cpx 'src/fonts/**' dist/fonts & webpack --env.mock --env.delayResponse=1000 -w",
    "dev:watch": "postcss src/css/*.css -d dist/css -w & cpx 'src/fonts/**' dist/fonts -w & webpack -w",
    "build": "NODE_ENV=production postcss src/css/*.css -d dist/css -x css --env & cpx 'src/fonts/**' dist/fonts & webpack --env.production",
    "lint": "tslint 'src/app/**/*.{ts,tsx} -c ./tslint.js --fix' & stylelint 'src/css/**/*.css' --fix",
    "test": "jest --collectCoverage --no-cache",
    "test:watch": "jest --watch",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "stats": "webpack --env.production --profile --json > stats.json",
    "analyze": "webpack --env.production --env.analyze"
  },
  "browserslist": [
    "last 2 versions",
    "Android >= 4.2",
    "iOS >= 9",
    "ie > 9"
  ]
